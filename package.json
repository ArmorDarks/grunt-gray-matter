{
  "name": "grunt-gray-matter",
  "version": "1.0.0",
  "description": "A Grunt task for extracting data header from file contents using Gray Matter",
  "homepage": "https://github.com/ArmorDarks/grunt-gray-matter",
  "author": "Serj Lavrin (https://github.com/ArmorDarks)",
  "repository": {
    "type": "git",
    "url": "git://github.com/ArmorDarks/grunt-gray-matter.git"
  },
  "bugs": {
    "url": "https://github.com/ArmorDarks/grunt-gray-matter/issues"
  },
  "license": "Apache-2.0",
  "keywords": [
    "grunt",
    "javascript",
    "es6",
    "gray-matter",
    "front-matter"
  ],
  "dependencies": {
    "chalk": "1.1.3",
    "gray-matter": "2.1.0",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "babel-jest": "18.0.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-latest": "^6.22.0",
    "grunt": "1.0.1",
    "grunt-contrib-clean": "^1.0.0",
    "jest": "18.1.0",
    "jit-grunt": "^0.10.0",
    "time-grunt": "^1.4.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "test": "grunt && jest"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "coffee",
      "js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.coffee$": "<rootDir>/utils/jest-preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(coffee|js)?$"
  }
}
